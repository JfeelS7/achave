generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Cliente {
  id        String   @id @default(uuid())
  nome      String
  email     String   @unique
  telefone  String   @unique(map: "Cliente_telefone")
  codigo    String   @unique
  createdAt DateTime @default(now())
  pago      Boolean  @default(false)
}
